{
  "name": "CScriptSIT Cool Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editor.selectionBackground": "#264f78",
    "editorCursor.foreground": "#ffffff",
    "editorWhitespace.foreground": "#3b3b3b"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.decimal",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Control Keywords Bold",
      "scope": "keyword.control.bold.cscriptsit",
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Test Keywords Bold",
      "scope": "entity.name.function.bold.test.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Verification Keywords Bold",
      "scope": "entity.name.function.bold.verification.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Analyzer Keywords Bold",
      "scope": "entity.name.function.bold.analyzer.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Action Keywords Bold",
      "scope": "entity.name.function.bold.action.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Data Keywords Bold",
      "scope": "entity.name.function.bold.data.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Utility Keywords Bold",
      "scope": "entity.name.function.bold.utility.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "MFG Keywords Bold",
      "scope": "entity.name.function.bold.mfg.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CS Keywords Bold",
      "scope": "entity.name.function.bold.cs.cscriptsit",
      "settings": {
        "foreground": "#4EC9B0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": [
        "variable.other"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": [
        "constant.language.boolean.cscriptsit"
      ],
      "settings": {
        "foreground": "#DCDCAA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.other.special.cscriptsit"
      ],
      "settings": {
        "foreground": "#DCDCAA",
        "fontStyle": "bold"
      }
    }
  ]
}